<%- include("partials/head", { arc, bot, user, path, title: "Soda - Dashboard" }) %>
<div class="content">

<h2>CHỌN MỘT MÁY CHỦ CỦA BẠN</h2>

<% if ('https://discord.gg/2mHgQQz3GN') { %>
    <div style="width: 512px;
   padding: 16px;
   margin: 8px;  background-color: rgba(77, 77, 255, 0.05);">
      <h3 style="color: #00FF66;   margin-top: 0;
   margin-bottom: 8px;
   font-size: 18px;
   font-family: 'Poppins', sans-serif;">Hey <%= user.username%>!</h3>
      <p style="margin: 0;
   font-size: 14px;
   font-family: 'Open Sans', sans-serif;
   color: #555;">Join the Soda-chan village if you haven't already! <a href="/support">Join Here!</a></p>
    </div>
<% } %>

<div class="servers">
<% user.guilds.forEach(guild => {
const guild_perms = new perms(guild.permissions.toString())
if(!guild_perms.has("MANAGE_GUILD")) return;
%>

<% if (guild.icon) {
 iconurl = "https://cdn.discordapp.com/icons/" + guild.id + "/" + guild.icon + ".png??size=512";
} else {
 iconurl = "https://cdn.discordapp.com/attachments/893953110349852686/970760403976093716/soda_chan_new.png";
}
%>

 
  <% if (bot.guilds.cache.get(guild.id)) { %>
  <div class="server">
   <div class="icon-and-name">
<img class="server-icon" src="<%= iconurl %>" alt="<% guild.name %>"/>
  <p class="guild-name"><%= guild.name %></p>
  </div>
    <a class="edit button" href="/dashboard/<%= guild.id %>">Dashboard</a>
</div>
  <% } %>

  <% }); %>
<% user.guilds.forEach(guild => {
const guild_perms = new perms(guild.permissions.toString())
if(!guild_perms.has("MANAGE_GUILD")) return;
%>

<% if (guild.icon) {
 iconurl = "https://cdn.discordapp.com/icons/" + guild.id + "/" + guild.icon + ".png??size=512";
} else {
 iconurl = "https://cdn.discordapp.com/attachments/893953110349852686/970760403976093716/soda_chan_new.png";
}
%>
%>

 
  <% if (!bot.guilds.cache.get(guild.id)) { %>
  <div class="server">
   <div class="icon-and-name">
<img class="server-icon" src="<%= iconurl %>" alt="<% guild.name %>"/>
  <p class="guild-name"><%= guild.name %></p>
  </div>
 <a href='<%= `${https}discordapp.com/oauth2/authorize?client_id=767705905235099658&scope=bot&guild_id=${guild.id}&response_type=code&redirect_uri=${encodeURIComponent(`${domain}/window`)}&permissions=490073207` %>' class="add button">Add Bot</a>
</div>
  <% } %>

  <% }); %>
</div>



